		# Speed of the iron auto-hammer as ticks taken to process a block
		# Default: 50
		# Range: 1 ~ 100000
		stone_hammer_speed: 50
	}
	basaltgen: {
		# The delay between each cobble generation in ticks
		# Default: 20
		# Range: 1 ~ 2147483647
		basaltgen_tick_rate: 20
		
		# Amount of cobble the diamond basaltgen produces per tick
		# Default: 32
		# Range: 1 ~ 1000
		diamond_basaltgen_amount: 32
		
